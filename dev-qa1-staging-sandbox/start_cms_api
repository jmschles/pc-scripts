cms_api_pid=`screen -ls | grep cms_api | awk -F '.' '{print $1}'`
if [ -z "$cms_api_pid" ]
then
  current_dir=`pwd`
  cd ~/cms_api/current
  screen -S cms_api -d -m bundle exec unicorn -p 3001
  cd $current_dir
  echo "cms_api started"
else
  echo "could not start cms_api: app is already running"
fi
