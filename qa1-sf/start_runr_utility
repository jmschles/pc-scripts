runr_util_pid=`screen -ls | grep runr_util | awk -F '.' '{print $1}'`
if [ -z "$runr_util_pid" ]
then
  current_dir=`pwd`
  cd ~/runr_utility/current
  screen -S runr_util -d -m bundle exec unicorn -p 3000
  cd $current_dir
  echo "runr_utility started"
else
  echo "could not start runr_utility: app is already running"
fi
